{
    "$schema": "https://schemas.cute.engineering/stable/cutekit.manifest.component.v1",
    "id": "navy-x86_64",
    "type": "target",
    "props": {
        "toolchain": "clang",
        "arch": "x86_64",
        "bits": "64",
        "sys": "navy",
        "abi": "sysv",
        "encoding": "utf8",
        "freestanding": false,
        "host": false
    },
    "routing": {
        "stdc-math": "ce-libm"
    },
    "tools": {
        "cc": {
            "cmd": "{shell.latest('clang')}",
            "args": [
                "-fblocks",
                "-target",
                "x86_64-none-elf",
                "-Wno-main-return-type"
            ]
        },
        "cxx": {
            "cmd": "{shell.latest('clang++')}",
            "args": [
                "-target",
                "x86_64-none-elf"
            ]
        },
        "ld": {
            "cmd": "{shell.latest('ld.lld')}",
            "args": [
                "-m",
                "elf_x86_64",
                "-T",
                "meta/targets/navy-x86_64.ld",
                "-z",
                "max-page-size=0x1000"
            ]
        },
        "ar": {
            "cmd": "{shell.latest('llvm-ar')}",
            "args": [
                "rcs"
            ]
        },
        "as": {
            "cmd": "{shell.latest('clang')}",
            "args": [
                "-target",
                "x86_64-none-elf",
                "-c"
            ]
        }
    }
}